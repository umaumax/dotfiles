snippet bats
	#!/usr/bin/env bats${0}

snippet test
	@test "${1:test something}" {
	  $0
	}

snippet line
	[ "${lines[${1:0}]}" = "${2:some value}" ]${0}

snippet output
	[ "$output" = "${1:foo: no such file 'nonexistent_filename'}" ]${0}

snippet status
	[ "$status" -eq ${1:0} ]${0}

snippet run
	run foo
	[ "$status" -eq ${1:0} ]${0}

{
    "title": "For benefit control key",
    "rules": [{
        "description": "Change caps_lock to left_control",
        "manipulators": [{
            "type": "basic",
            "from": {
                "key_code": "caps_lock",
                "modifiers": {
                    "optional": [
                        "any"
                    ]
                }
            },
            "to": [{
                "key_code": "left_control"
            }]
        }]
    }]
}

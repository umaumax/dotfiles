{
  "title": "For avoid miss windows minimization",
  "rules": [
    {
      "description": "Change Ctrl+m to Enter Key",
      "manipulators": [
        {
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["left_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": ["right_command"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}

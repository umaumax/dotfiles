{
  "explorer.confirmDragAndDrop": false,
  "editor.copyWithSyntaxHighlighting": false,
  "window.zoomLevel": -1,
  "workbench.colorCustomizations": {
    "editor.selectionHighlightBackground": "#f002f088"
  },
  "workbench.colorCustomizations": {
    "tab.activeBorder": "#2266ff",
    "tab.unfocusedActiveBorder": "#000000"
  },
  "editor.rulers": [
    80,160,240,320
  ],
  "files.insertFinalNewline": true,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.showExitAlert": false,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.smoothScrolling": true,
  "editor.formatOnSave": false,
  "[csharp]": {
    "editor.formatOnSave": true,
    "editor.codeLens": false
  },
  "[typescript]": {
    "editor.formatOnSave": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "files.exclude": {
    "**/*.exe": true,
    "**/*.meta": true
  },
  "terminal.integrated.env.osx": {
    "PATH": "$PATH:$HOME/.dotnet:/usr/local/bin/"
  },
  "omnisharp.useGlobalMono": "always",
  "omnisharp.monoPath": "/usr/local/opt/mono/",
  "auto-correct.dictionary": [
    {
      "languages": ["*"],
      "words": {
        "+>": "=>",
        "warn": "WARN:",
        "todo": "TODO:",
        "note": "NOTE:",
        "flaot": "float",
        "stuct": "struct",
        "pravite": "private",
        "{publci,pubcli}": "{public}",
        "hte": "the",
        "filed": "field",
        "mispell": "misspell",
        "mispelled": "misspelled",
        "{despa,sepe}rat{e,es,ed,ing,ely,ion,ions,or}": "{despe,sepa}rat{}"
      }
    }
  ],
  "plantuml.render": "PlantUMLServer",
  "plantuml.server": "https://www.plantuml.com/plantuml",
  "plantuml.commandArgs": []
}
